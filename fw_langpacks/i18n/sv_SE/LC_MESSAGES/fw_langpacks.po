msgstr "Förbereder att kopiera %s till %s"

#, php-format
msgid "No destination directory %s to copy %s to"
msgstr "Ingen destionationsmapp %s att kopiera %s till"

#, php-format
msgid "Updated %s"
msgstr "Uppdaterade %s"

