msgid "<b>.</b> = wildcard, matches one or more characters<br>"
msgstr "<b>.</b> = один или несколько любых символов<br>"

msgid "<b>N</b> = matches any digit from 2-9<br>"
msgstr "<b>N</b> = любому числу от 2 до 9<br>"

