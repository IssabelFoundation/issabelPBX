msgstr "Gravações do sistema"

msgid "Add System Recording"
msgstr "Adicionar gravação do sistema"

msgid "Step 1"
msgstr "Passo 1"

msgid "Record or upload"
msgstr "Grabar o subir"

msgid "Step 2"
msgstr "Passo 2"

